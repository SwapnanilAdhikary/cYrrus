# Copy this file to .env and fill in your actual values

# Gemini API Key (get from Google AI Studio)
GEMINI_API_KEY=your_gemini_api_key_here

# Google Cloud Project Configuration
GCP_PROJECT_ID=your_project_id_here
GCP_REGION=us-central1
GCP_MODEL=gemini-2.5-pro

# Path to your GCP service account credentials file
GCP_CREDENTIALS_PATH=./credentials/vertex-credentials.json
OCR_CREDENTIALS_PATH=./credentials/ocr-credentials.json
